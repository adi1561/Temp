class Parent {
    
    private Double money;
    
}
class child extends Parent {
    
    System.out.println(money);
}
public class Main
{
	public static void main(String[] args) {
	
	            child ch  = new child();
	            
	            System.out.println(ch);
	            
	
		}
}


// money is private virable cant be inhirated to the chaild class
